  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeaRyanC/rtrace.git"
  },
  "keywords": [
    "rust",
    "nodejs",
    "bindings"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SeaRyanC/rtrace/issues"
  },
  "homepage": "https://github.com/SeaRyanC/rtrace#readme",
  "napi": {
    "name": "rtrace",
    "triples": {
      "defaults": true
    }
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.0.0"
  }
}